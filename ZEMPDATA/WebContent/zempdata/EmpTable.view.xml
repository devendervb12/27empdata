<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
		controllerName="zempdata.EmpTable" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Title">
		<content>
	         <Table items="{/empInfo}">
	            <columns>
	            
	               <Column>
	                    <Text text="{r2>EMP_NUM}"/>
	               </Column>
	                <Column>
	                    <Text text="{r2>NAME}"/>
	               </Column>
	                <Column>
	                    <Text text="{r2>ROLE}"/>
	               </Column>
	                <Column>
	                    <Text text="{r2>DEPT}"/>
	               </Column>
	            </columns>
	            <items>
	              <ColumnListItem>
	                    <cells>
	                        <Text text="{code}"/>
	                        <Text text="{name}"/>
	                        <Text text="{role}"/>
	                        <Text text="{dept}"/>
	                    </cells>
	               </ColumnListItem>
	            </items>
	         </Table>
		</content>
	</Page>
</core:View>